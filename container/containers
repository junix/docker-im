#!/usr/bin/env python

import utils
from container import Container

if __name__ == '__main__':
    cmd = 'docker ps -a'
    containers = utils.exec_cmd(cmd).split('\n')
    title = containers[0]
    names_offset = title.index('NAMES')
    cs = [c[names_offset:] for c in containers[1:] if c]
    cs.sort()
    for c in cs:
        print(c)
